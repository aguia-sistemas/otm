<html class="loading" data-textdirection="ltr" lang="en">
<!-- BEGIN: Head-->

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <meta name="description" content="OTM">
    <meta name="keywords" content="otm">
    <meta name="author" content="Hemerson Camargo">
    <title>@ViewData["Title"] | Otm</title>
    <link rel="apple-touch-icon" href="favicon/apple-touch-icon-152x152.png">
    <link rel="shortcut icon" type="image/x-icon" href="/img/robot.ico">
    <link href="~/css/template/icon.css" rel="stylesheet">
    <!-- BEGIN: VENDOR CSS-->
    <link rel="stylesheet" type="text/css" href="~/css/template/vendors.css">
    <link rel="stylesheet" type="text/css" href="~/css/template/flag-icon.css">
    <!-- END: VENDOR CSS-->
    <!-- BEGIN: Page Level CSS-->
    <link rel="stylesheet" type="text/css" href="~/css/template/materialize.css">
    <link rel="stylesheet" type="text/css" href="~/css/template/style.css">
    <link rel="stylesheet" type="text/css" href="~/css/template/app-file-manager.css">
    <link rel="stylesheet" type="text/css" href="~/css/template/widget-timeline.css">
    <link rel="stylesheet" type="text/css" href="~/css/template/charts-sparklines.css">
    <!-- END: Page Level CSS-->
    <!-- BEGIN: Custom CSS-->
    <link rel="stylesheet" type="text/css" href="~/css/template/custom.css">
    <!-- END: Custom CSS-->
</head>
<!-- END: Head-->

<body class="vertical-layout vertical-menu-collapsible page-header-dark vertical-modern-menu 2-columns app-page" data-open="click" data-menu="vertical-modern-menu" data-col="2-columns" style="">

    <!-- BEGIN: SideNav-->
        <aside class="sidenav-main nav-expanded nav-lock nav-collapsible sidenav-light sidenav-active-square">
        <div class="brand-sidebar">
            <h1 class="logo-wrapper">
            <a class="brand-logo darken-1" href="~/">
                <img class="icon-med-and-up hide-on-med-and-down waves-effect waves-light" src="~/img/robot.svg" alt="otm logo">
                <img class="icon-medium-and-down show-on-medium-and-down hide-on-med-and-up" src="~/img/robot.svg" alt="otm logo">
                <span class="logo-text hide-on-med-and-down">OTM</span>
            </a>
        </h1>
        </div>
        
        <vc:main-menu></vc:main-menu>

        <div class="navigation-background"></div><a class="sidenav-trigger btn-sidenav-toggle btn-floating btn-medium waves-effect waves-light hide-on-large-only" href="#" data-target="slide-out"><i class="material-icons">menu</i></a>
    </aside>
    <!-- END: SideNav-->

    <!-- BEGIN: Page Main-->
    <div id="main">
        <div class="row">
            <div class="content-wrapper-before gradient-45deg-indigo-blue"></div>

            <div class="breadcrumbs-dark pb-0" id="breadcrumbs-wrapper">
                <!-- Search for small screen-->
                <div class="container">
                    <div class="row">
                        <div class="col s10 m6 l6">
                            <h5 class="breadcrumbs-title mt-0 mb-0"><span>@ViewData["Title"]</span></h5>
                            <ol class="breadcrumbs mb-0">
                                <li class="breadcrumb-item"><a href="index.html">@((ViewData["breadcrumbs"] as string[])[0])</a>
                                </li>
                                <li class="breadcrumb-item active"><a href="#">@((ViewData["breadcrumbs"] as string[])[1])</a>
                                </li>
                            </ol>  
                        </div>
                    </div>
                </div>
            </div>

            <div class="col s12">
                <div class="container">
                    @RenderBody()
                </div>
                <div class="content-overlay"></div>
            </div>
        </div>
    </div>
    <!-- END: Page Main-->
    
    <!-- BEGIN: Footer
    <footer class="page-footer footer gradient-shadow navbar-border navbar-shadow footer-light footer-static">
        <div class="footer-copyright">
            <div class="container">
                <span>© 2020<a href="http://themeforest.net/user/pixinvent/portfolio?ref=pixinvent" target="_blank">AGUIA SISTEMAS DE ARMAZENAGEM</a> 
                All rights reserved.</span>
                <span class="right hide-on-small-only">Design and Developed by <a href="https://pixinvent.com/">HEMERSON CAMARGO</a></span>
            </div>
        </div>
    </footer>
    END: Footer-->

    <!-- BEGIN VENDOR JS-->
    <script src="~/js/template/vendors.js"></script>
    <!-- BEGIN VENDOR JS-->

    <!-- BEGIN THEME  JS-->
    <script src="~/js/template/plugins.js"></script>
    <script src="~/js/template/search.js"></script>
    <script src="~/js/template/customizer.js"></script>

    
    <!-- END THEME  JS-->

    <!-- BEGIN PAGE LEVEL JS-->
    @RenderSection("Scripts", required: false)
    <!-- END PAGE LEVEL JS-->

    <div class="sidenav-overlay" style="display: none;"></div>
    <div class="drag-target"></div>
    <div class="sidenav-overlay"></div>
    <div class="drag-target right-aligned"></div>
    <div class="sidenav-overlay"></div>
    <div class="drag-target right-aligned"></div>
    <div class="sidenav-overlay" style="display: none; opacity: 0;"></div>
    <div class="drag-target right-aligned"></div>
